{
  "name": "@6thquake/react-material",
  "version": "0.4.2",
  "private": false,
  "licenses": "MIT",
  "main": "index.js",
  "module": "index.es.js",
  "description": "React components that implement Google's Material Design.",
  "maintainers": [
    {
      "lico": "lico.atom@gmail.com"
    }
  ],
  "keywords": [
    "react",
    "react components",
    "react material",
    "material design",
    "material ui"
  ],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:6thquake/react-material.git"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build:es2015": "cross-env NODE_ENV=production babel --config-file ../../babel.config.js ./src --out-dir ./build --ignore *.test.js",
    "build:es2015modules": "cross-env NODE_ENV=production BABEL_ENV=modules babel --config-file ../../babel.config.js ./src/index.js --out-file ./build/index.es.js",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ../../babel.config.js ./src --out-dir ./build/es --ignore *.test.js",
    "build:umd": "cross-env BABEL_ENV=production-umd rollup -c scripts/rollup.config.js",
    "build:copy-files": "babel-node --config-file ../../babel.config.js ./scripts/copy-files.js",
    "build": "yarn build:es2015 && yarn build:es2015modules && yarn build:es && yarn build:umd && yarn build:copy-files",
    "release": "yarn build && npm publish build",
    "deploy:build": "npm run build && npm publish build --tag latest",
    "deploy": "npm publish build --tag latest"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "7.0.0",
    "@date-io/moment": "^1.1.0",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^4.0.0-alpha.0",
    "@material-ui/lab": "^3.0.0-alpha.30",
    "@material-ui/utils": "^3.0.0-alpha.3",
    "@types/jss": "^9.5.3",
    "@types/react-transition-group": "^2.0.8",
    "brcast": "^3.0.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "csstype": "^2.5.2",
    "debounce": "^1.1.0",
    "deepmerge": "^2.0.1",
    "dom-helpers": "^3.2.1",
    "formsy-react": "^1.1.4",
    "hoist-non-react-statics": "^2.5.0",
    "immutability-helper": "^2.7.0",
    "is-plain-object": "^2.0.4",
    "jss": "^9.3.3",
    "jss-camel-case": "^6.0.0",
    "jss-default-unit": "^8.0.2",
    "jss-global": "^3.0.0",
    "jss-nested": "^6.0.1",
    "jss-props-sort": "^6.0.0",
    "jss-vendor-prefixer": "^7.0.0",
    "keycode": "^2.1.9",
    "lodash": "^4.2.0",
    "material-ui-pickers": "^2.2.1",
    "moment": "^2.24.0",
    "normalize-scroll-left": "^0.1.2",
    "popper.js": "^1.14.1",
    "prismjs": "^1.14.0",
    "prop-types": "^15.7.2",
    "rc-menu": "^7.0.4",
    "rc-tree": "^1.12.0",
    "re-resizable": "^4.4.8",
    "react-addons-update": "^15.6.2",
    "react-annotation": "^1.2.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-event-listener": "^0.6.2",
    "react-jss": "^8.1.0",
    "react-lifecycles-compat": "^2.0.0",
    "react-resizable": "^1.7.5",
    "react-scrollbar-size": "^2.0.2",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.29.0",
    "scroll": "^2.0.3",
    "video.js": "^7.4.1",
    "warning": "^4.0.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=6.11.0"
  }
}
